{
    "name": "chinese-vocab-backend",
    "version": "0.0.1",
    "description": "Backend for Chinese vocabulary learning app",
    "private": true,
    "scripts": {
        "start": "nest start",
        "start:dev": "nest start --watch",
        "build": "nest build",
        "start:prod": "node dist/main.js",
        "seed": "ts-node src/seed.ts",
        "migration:generate": "npx typeorm-ts-node-commonjs -d ./data-source.ts migration:generate",
        "migration:run": "node run-migration.js",
        "migration:run:local": "cross-env POSTGRES_HOST=localhost POSTGRES_PORT=5432 POSTGRES_USER=postgres POSTGRES_PASSWORD=postgres POSTGRES_DB=postgres node run-migration.js"
    },
    "dependencies": {
        "@google-cloud/text-to-speech": "^6.2.0",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.1.1",
        "@nestjs/passport": "^10.0.2",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/throttler": "^6.4.0",
        "@nestjs/typeorm": "^10.0.0",
        "@types/pg": "^8.15.4",
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "dotenv": "^16.6.1",
        "helmet": "^8.1.0",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.16.3",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "sqlite3": "^5.1.6"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/passport-jwt": "^3.0.9",
        "@types/passport-local": "^1.0.35",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.2",
        "typeorm": "^0.3.25",
        "typescript": "^5.1.3"
    }
}
